<mujoco model="spot_default">
<!-- Copyright (c) 2023 Boston Dynamics AI Institute LLC. All rights reserved. -->

  <compiler angle="radian" assetdir="../../../meshes/spot/" autolimits="true"/>

  <option timestep="0.0005" iterations="50" solver="Newton" integrator="RK4" density="1">
  </option>

  <default>
    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="2"/>
    </default>
    <default class="collision">
      <geom group="3" friction="0.3" priority="4" rgba="1 1 1 1" solimp="0.9 0.95 0.001 0.5 2" solref="0.02 1">
      </geom>
    </default>
    <default class="corner_contact">
      <geom type="sphere" size="1e-7" friction="0.5" priority="3"/>
    </default>

    <!-- The arm damping is higher than the damping gains in spot-sim, because actual joint damping is unknown -->
    <default class="arm">
      <default class="arm_sh0">
        <joint damping="3.0" armature="0.025" range="-2.61799387799149441136 3.14159265358979311599"/>
      </default>
      <default class="arm_sh1">
        <joint damping="3.0" armature="0.025" range="-3.14159265358979311599 0.52359877559829881565"/>
      </default>
      <default class="arm_el0">
        <joint damping="3.0" armature="0.025" range="0 3.14159265358979311599"/>
      </default>
      <default class="arm_el1">
        <joint damping="3.0" armature="0.025" range="-2.79252680319092716487 2.79252680319092716487"/>
      </default>
      <default class="arm_wr0">
        <joint damping="3.0" armature="0.025" range="-1.83259571459404613236 1.83259571459404613236"/>
      </default>
      <default class="arm_wr1">
        <joint damping="3.0" armature="0.025" range="-2.87979326579064354163 2.87979326579064354163"/>
      </default>
      <default class="arm_f1x">
        <joint damping="3.0" armature="0.025" range="-1.57 0"/>
      </default>
    </default>

    <default class="leg">
      <default class="hip_x">
        <joint damping="3.0" armature="0.025" range="-0.785398 0.785398"/>
      </default>
      <default class="hip_y">
        <joint damping="3.0" armature="0.025" range="-0.898845 2.29511"/>
      </default>
      <default class="knee">
        <joint damping="3.0" armature="0.025" range="-2.7929 -0.254801"/>
      </default>
    </default>

  </default>
</mujoco>
