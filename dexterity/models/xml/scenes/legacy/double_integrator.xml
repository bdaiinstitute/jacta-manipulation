<mujoco model="DoubleIntegrator">
<!-- Copyright (c) 2023 Boston Dynamics AI Institute LLC. All rights reserved. -->

  <option timestep="0.005" />

  <default>
    <default class="slider">
      <position kp="100" ctrlrange="-1.8 1.8" forcerange="-10 10"/>
    </default>
  </default>

  <worldbody>
    <body name="cart" pos="0 0 0">
      <joint name="slider" damping="0" type="slide" axis="1 0 0" />
      <geom name="cart" type="box" size="0.25 0.1 0.1" mass="1"/>
      <site pos="0 0 0" name="site_cart"/>
    </body>
  </worldbody>

  <actuator>
    <position name="actuator_cart" joint="slider" class="slider" />
  </actuator>

</mujoco>
